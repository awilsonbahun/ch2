public class BreadTester {
    public static void main(String args[]){

        Bread bread=new Bread(3,4,2,1,1,2"flatbread");
        bread.getIngredients();
        bread.Recipe();
        bread.Bake();
        SourdoughBread s= new SourdoughBread(4,3,3,1,2,2"sourdoughbread");
        s.recipe();



    }
}
